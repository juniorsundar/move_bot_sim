<launch>
  <arg name="multi" default=""/>
  <arg name="laser_model"  default="sick-561"/>

  <arg name="test_suffix" value="$(arg multi)_$(arg laser_model)"/>
  <include file="$(find omni_base_description)/robots/upload.launch" pass_all_args="true"/>
  <include file="$(find urdf_test)/launch/test_urdf.launch">
    <arg name="get_description" value="true"/>
    <arg name="test_suffix" value="$(eval test_suffix.replace('-', '_'))"/>
  </include>
</launch>
